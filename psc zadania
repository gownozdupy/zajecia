		case PIERWSZY:
			if (isupper(znak)) {
				stan = KOLEJNY;
				printf("\nMasz na imie %c", znak);
			}
			else if (islower(znak)) {
				printf("Imie nalezy pisac wielka litera.\n");
				stan = BLAD;
			}
			else if (!isalpha(znak) && ('\n' != znak)) {
				printf("Imie sklada sie tylko z liter.\n");
				stan = BLAD;
			}
